{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/videos/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "video/mp4"
      },
      "continue": true
    }
  ]
} 